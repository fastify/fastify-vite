{
  "name": "example-vue-vanilla-ts",
  "type": "module",
  "scripts": {
    "dev": "tsx src/server.ts --dev",
    "start": "node dist/server.js",
    "build": "pnpm build:client && pnpm build:server",
    "build:server": "vue-tsc && tsdown -d ./dist src/server.ts",
    "build:client": "vite build --emptyOutDir",
    "lint": "oxlint",
    "test": "tsx --test"
  },
  "dependencies": {
    "@fastify/vite": "workspace:^",
    "fastify": "catalog:",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@types/node": "^22.13.17",
    "@vitejs/plugin-vue": "catalog:",
    "oxlint": "catalog:",
    "tsdown": "^0.9.1",
    "tsx": "^4.19.3",
    "vite": "catalog:",
    "vue-tsc": "^2.2.8"
  }
}