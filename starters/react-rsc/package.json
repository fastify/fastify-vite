    "@hiogawa/utils": "^1.7.0",
    "fastify": "^5.2.2",
    "history": "^5.3.0",
    "minipass": "^7.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.0",
    "react-router-dom": "^7.5.0",
    "react-server-dom-webpack": "^19.1.0",
    "unihead": "^0.8.0",
    "valtio": "^2.1.4"
