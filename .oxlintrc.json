{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "categories": {
    "correctness": "error"
  },
  "plugins": ["import", "typescript"],
  "rules": {
    "import/extensions": [
      "error",
      "always",
      {
        "ignorePackages": true
      }
    ],
    "no-unused-vars": "off",
    "typescript/consistent-type-definitions": "error"
  }
}
