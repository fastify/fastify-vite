{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "categories": {
    "correctness": "error"
  },
  "plugins": ["eslint", "import", "oxc", "typescript", "unicorn"],
  "rules": {
    "import/extensions": [
      "error",
      "always",
      {
        "ignorePackages": true
      }
    ],
    "no-unused-vars": "warn",
    "typescript/consistent-type-definitions": "error"
  }
}
