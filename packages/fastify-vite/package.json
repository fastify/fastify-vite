{
  "name": "@fastify/vite",
  "version": "8.4.1",
  "description": "Official Fastify plugin for Vite integration.",
  "keywords": [
    "fastify",
    "vite"
  ],
  "homepage": "https://fastify.github.io/fastify-vite/",
  "bugs": {
    "url": "https://github.com/fastify/fastify-vite/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/fastify-vite.git"
  },
  "files": [
    "config.js",
    "html.js",
    "index.js",
    "ioutils.cjs",
    "plugin.mjs",
    "setup.js",
    "utils.js",
    "mode/development.js",
    "mode/production.js",
    "types/index.d.ts",
    "types/plugin.d.ts",
    "types/utils.d.ts"
  ],
  "main": "index.js",
  "types": "./types/index.d.ts",
  "typings": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./index.js",
      "require": "./index.js"
    },
    "./utils": {
      "types": "./types/utils.d.ts",
      "import": "./utils.js",
      "require": "./utils.js"
    },
    "./plugin": {
      "types": "./types/plugin.d.ts",
      "import": "./plugin.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest run --no-file-parallelism && vitest --typecheck.only --run"
  },
  "dependencies": {
    "@fastify/deepmerge": "^3.1.0",
    "@fastify/middie": "catalog:",
    "@fastify/static": "^9.0.0",
    "fastify-plugin": "^5.1.0",
    "fs-extra": "^11.3.3",
    "html-rewriter-wasm": "catalog:",
    "klaw": "^4.1.0",
    "package-directory": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "fastify": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "fastify": ">=5",
    "vite": ">=6"
  },
  "deprecated": false
}
